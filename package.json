{
  "name": "physicshub",
  "homepage": "https://physicshub.github.io",
  "private": true,
  "version": "2.7.3",
  "type": "module",
  "scripts": {
    "dev": "vite & nodemon --watch src/routes.js --watch public/index.html --exec \"npm run generate:assets\"",
    "build": "vite build && npm run generate:assets",
    "preview": "npm run build && vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "generate:sitemap": "node scripts/sitemap-generator.js",
    "copy:404": "node scripts/copy-404.js",
    "generate:assets": "npm run generate:sitemap && npm run copy:404",
    "contributors": "ts-node scripts/contributors/contributors.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "clsx": "^2.1.1",
    "katex": "^0.16.25",
    "motion": "^12.23.24",
    "p5": "^2.0.5",
    "planck": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.2.0",
    "react-katex": "^3.1.0",
    "react-router-dom": "^7.9.4",
    "react-router-hash-link": "^2.4.3",
    "react-syntax-highlighter": "^16.1.0",
    "xml-formatter": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.10.0",
    "@types/p5": "^1.7.6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^5.0.4",
    "axios": "^1.13.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-xml-parser": "^5.3.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "nodemon": "^3.1.10",
    "puppeteer": "^24.29.0",
    "sitemap": "^8.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "author": "MattQ",
  "license": "MIT"
}
